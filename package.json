{
  "name": "maptalks-gl-vue",
  "version": "1.4.0-alpha.2",
  "description": "A Vue 3 map component library based on Maptalks GL, supporting multiple map sources such as Sky Map, Google Map, OpenStreetMap, etc.",
  "type": "module",
  "main": "./dist/maptalks-gl-vue.umd.js",
  "module": "./dist/maptalks-gl-vue.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/maptalks-gl-vue.es.js",
      "require": "./dist/maptalks-gl-vue.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "assets",
    "README.md",
    "CONTRIBUTING.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "vue-tsc && vite build",
    "example:dev": "cd example && npm run dev",
    "example:build": "cd example && npm run build",
    "example:preview": "cd example && npm run preview",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:release": "changeset publish",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "ant-design-vue": "^4.2.6",
    "maptalks-gl": "^0.118.1",
    "vue": "^3.5.24"
  },
  "dependencies": {
    "@vueuse/core": "^14.1.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "ant-design-vue": "^4.2.6",
    "husky": "^8.0.3",
    "maptalks-gl": "^0.118.1",
    "sass": "^1.94.2",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vue": "^3.5.24",
    "vue-tsc": "^3.1.4"
  },
  "keywords": [
    "vue",
    "vue3",
    "map",
    "maptalks",
    "maptalks-gl",
    "gis",
    "webgis",
    "tianditu",
    "google-maps",
    "openstreetmap"
  ],
  "author": "chenyyz1015 <chenyyz1015@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chenyyz1015/maptalks-gl-vue.git"
  },
  "bugs": {
    "url": "https://github.com/chenyyz1015/maptalks-gl-vue/issues"
  },
  "homepage": "https://github.com/chenyyz1015/maptalks-gl-vue#readme"
}
